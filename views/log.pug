extends layouts/_layout.pug

block title
	title Logs - Tagpro.dev


block content
	main#main
		header
			h2 #{title}

		section
			table
				thead
					tr
						th EUID
						th Date
						th.text-c Season
						th.text-c Average ELO
						th.text-c Duration
						th.text-c Red
						th.text-c Blue
						th.text-c Winner

				tbody
					each n, key in games
						- var href= 'https://tagpro.eu/?match=' + n.euid
						tr
							td
								a(href=`${href}`)= n.euid

							td= n.date
							td.text-c= n.season
							td.text-c= n.elo
							td.text-c= n.duration
							td.text-c= n.red
							td.text-c= n.blue
							td.text-c= n.winner
