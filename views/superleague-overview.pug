					- let max = parseInt(mvb[0].value)
					each n, key in mvb
						.row(class=(key === 0 ? 'top' : ''))
							.item.rank= n.rank
							.item.team(
								data-position= i
								style="--teamcolor: " + n.color + "; --teamglow: " + n.color + "80"
							)= n.acronym
							.item.player
								a(
									href='#'
								)= n.player

							// .item.value=  n.value
							- let percent = ((n.value / max) * 100).toFixed(0)
							.item.test
								.v= n.value
								// .v= percent
								.bar
									.inner(style="width:" + percent + "%")




